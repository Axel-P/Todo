{
  "name": "todo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "npm run graphql:generate-types && react-native run-ios",
    "web": "npm run graphql:generate-types && react-scripts start",
    "start": "npm run graphql:generate-types && react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "graphql:generate-types": "rm -r src/types/graphql/ && apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql --outputFlat src/types/graphql",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@react-navigation/stack": "^6.0.11",
    "@types/i18n": "^0.13.2",
    "@types/styled-components": "^5.1.19",
    "@types/styled-components-react-native": "^5.1.3",
    "i18next": "^21.6.5",
    "i18next-browser-languagedetector": "^6.1.2",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "react-native": "0.66.4",
    "react-native-gesture-handler": "1.10.3",
    "react-native-reanimated": "2.2.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-web": "^0.17.5",
    "react-navigation": "^4.4.4",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^12.1.5",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
